  <button onclick="myFunction()">CLICK ME</button> 
<script> 
function myFunction() { 
  var btn = document.createElement("BUTTON"); 
  btn.innerHTML = "CLICK ME‚Äù; 
  btn.setAttribute("id", "btn"); 
  document.body.appendChild(btn); 
} 
</script> 
